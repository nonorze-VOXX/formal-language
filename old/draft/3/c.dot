graph robotFramework {
    fontsize = 20;
    fontname = "Helvetica,Arial,sans-serif";
    node [fontname = "Helvetica,Arial,sans-serif";];
    edge [fontname = "Helvetica,Ariasans-serif"; dir = forward;];
    node [shape = circle;];
    
    start -- q0 [dir = forward;];
    // q0 -- q1 [label = "1";];
    q0 -- q0 [label = <&Sigma;>;];
    q0 -- q1 [label = <&epsilon;>;];
    q0 -- q3 [label = <&epsilon;>;];
    
    
    q1 -- q2 [label = "1";];
    q2 -- q1 [label = "1";];
    
    q3 -- q4 [label = "0";];
    q4 -- q5 [label = "0";];
    q5 -- q6 [label = "0";];
    q6 -- q6 [label = <&Sigma;>;];
    
    q2 [peripheries = 2;];
    q5 [peripheries = 2;];
}